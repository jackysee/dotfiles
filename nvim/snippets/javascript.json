{
    "no-console": {
        "prefix": "noc",
        "body": [
            "/* eslint-disable no-console */",
            "console.log($1);"
        ]
    },
    "For Range": {
        "prefix": "forr",
        "body": [
            "for (let ${1:index} = 0; ${1:index} < ${2:number}; ${1:index}++) {",
                "\t$0",
                "}"
        ],
        "description": "For Range"
    },
    "import vue" : {
        "prefix": "iv",
        "body": "import { $1 } from 'vue';"
    },
}
