{
    "console.log": {
        "prefix": "cl",
        "body": [
            "console.log($1);"
        ]
    },
    "no-console": {
        "prefix": "noc",
        "body": [
            "/* eslint-disable no-console */",
            "console.log($1);"
        ]
    },
    "For Range": {
        "prefix": "forr",
        "body": [
            "for (let ${1:index} = 0; ${1:index} < ${2:number}; ${1:index}++) {",
                "\t$0",
                "}"
        ],
        "description": "For Range"
    },
    "import vue" : {
        "prefix": "iv",
        "body": "import { $1 } from 'vue';"
    },
    "import pinia": {
        "prefix": "ip",
        "body": "import { $1 } from 'pinia';"
    },
    "import vue-router" :{
        "prefix": "ir",
        "body": "import { $1 } from 'vue-router';"
    },
    "import constants": {
        "prefix": "ics",
        "body": "import { $1 } from '@/constant';"
    },
    "import message": {
        "prefix": "ims",
        "body": "import { getMessage as \\$t$1 } from '@/utils/message';"
    }
}
