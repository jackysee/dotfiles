{
    "import @/components": {
        "prefix":"ico",
        "body": "import $1 from '@/components/$1.vue';"
    },
    "import common-vue" : {
        "prefix": "icv",
        "body": "import $1 from 'common-vue/$1';"
    },
    "import common-vue3" : {
        "prefix": "icvv",
        "body": "import $1 from 'common-vue3/$1';"
    },
    "import vue" : {
        "prefix": "iv",
        "body": "import { $1 } from 'vue';"
    },
    "import pinia": {
        "prefix": "ip",
        "body": "import { $1 } from 'pinia';"
    },
    "import vue-router" :{
        "prefix": "ir",
        "body": "import { $1 } from 'vue-router';"
    },
    "vue sfc": {
        "prefix":"vvue",
        "body": [
            "<template>",
            "    <div>",
            "        $1",
            "    </div>",
            "</template>",
            "",
            "<script>",
            "",
            "export default {",
            "    $2",
            "};",
            "</script>"
        ]
    },
    "vue setup": {
        "prefix":"vsu",
        "body": [
            "<script setup>",
            "$1",
            "</script>",
            "",
            "<template>",
            "    <div>",
            "        $2",
            "    </div>",
            "</template>"
        ]
    },
    "histoire story": {
        "prefix":"vst",
        "body": [
            "<script setup>",
            "$1",
            "</script>",
            "",
            "<template>",
            "    <Story title='$2'>",
            "        $3",
            "    </Story>",
            "</template>"
        ]
    },
    "message $t": {
        "prefix":"tt",
        "body":"{{\\$t('$0')}}"
    }
}
