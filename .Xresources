
*saveLines: 16384

Xft*antialias : true
Xft*hinting   : 1
Xft*dpi       : 96

! *.font: xft:dejavu sans mono:size=9:antialias=true
! *.boldFont: xft:dejavu sans mono:bold:size=9:antialias=true
! *.italicFont: xft:dejavu sans mono:italic:size=9:antialias=true
! *.boldItalicFont: xft:dejavu sans mono:bold:italic:size=9:antialias=true

URxvt.lineSpace: 0
URxvt.interalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.scrollBar_right: false
URxvt.scrollBar: false
URxvt.urgentOnBell: true
URxvt.font: xft:dejavu\ sans\ mono-9:antialias=true
URxvt.letterSpace: -1
URxvt*termName: rxvt-unicode-256color
! URxvt.perl-ext-common: font-size
! URxvt.keysym.C-Up:     font-size:increase
! URxvt.keysym.C-Down:   font-size:decrease
! URxvt.keysym.C-S-Up:   font-size:incglobal
! URxvt.keysym.C-S-Down: font-size:decglobal
! URxvt.keysym.C-equal:  font-size:reset
! URxvt.keysym.C-slash:  font-size:show


! xterm*faceName: Dejavu Sans Mono
! xterm*faceName: Monaco
! xterm*faceName: Fira Code
xterm*faceName: Menlo
xterm*faceNameDoublesize: Noto Sans CJK TC
xterm*faceSize: 12
xterm*faceSize1: 9
xterm*faceSize2: 11
xterm*faceSize3: 12
xterm*faceSize4: 13
xterm*faceSize5: 16
xterm*faceSize6: 18
xterm*vt100*geometry: 120x40
xterm*cursorBlink: True
xterm*internalBorder: 0
xterm*VT100.Translations: #override \
        Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
        Ctrl Shift <Key>C:    copy-selection(CLIPBOARD) \n\
        Ctrl <Key> minus: smaller-vt-font() \n\
        Ctrl <Key> plus: larger-vt-font() \n\
        Ctrl <Key> KP_Subtract: smaller-vt-font() \n\
        Ctrl <Key> KP_Add: larger-vt-font()


